{
    "name": "bryanjamesmiller/laravel-inky",
    "description": "Foundation Inky email templates in Laravel",
    "keywords": ["foundation", "inky", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bryan Miller",
            "email": "bryan@kirschbaumdevelopment.com"
        }
    ],
    "require": {
      "incentfit/inky": "1.3.6.5",
      "illuminate/support": "^5.7|^6.0",
      "illuminate/view": "^5.7|^6.0",
      "symfony/css-selector": "^2.7|^3.0|^4.0",
      "symfony/dom-crawler": "^2.7|^3.0|^4.0",
      "tijsverkoyen/css-to-inline-styles": "^2.2"
    },
    "require-dev": {
      "phpunit/phpunit": "^8.0",
      "graham-campbell/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Bryanjamesmiller\\LaravelInky\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bryanjamesmiller\\Tests\\LaravelInky\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bryanjamesmiller\\LaravelInky\\InkyServiceProvider"
            ]
        }
    }
}
